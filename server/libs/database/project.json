{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "server/libs/database/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/server/libs/database",
        "main": "server/libs/database/src/main.ts",
        "tsConfig": "server/libs/database/tsconfig.app.json",
        "assets": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "server/libs/database/src/environments/environment.ts",
              "with": "server/libs/database/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "database:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["server/libs/database/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/server/libs/database"],
      "options": {
        "jestConfig": "server/libs/database/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
